{
  "name": "feature-flags-backend",
  "version": "1.0.0",
  "description": "Feature flags backend with AWS AppConfig",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "typecheck": "tsc --noEmit",
    "dev": "npm run build && npm run lint && npm run test"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^2.27.0",
    "@aws-lambda-powertools/metrics": "^2.27.0",
    "@aws-sdk/client-appconfig": "^3.0.0",
    "@aws-sdk/client-appconfigdata": "^3.0.0",
    "@aws-sdk/client-dynamodb": "^3.0.0",
    "@middy/core": "^6.4.5",
    "@middy/http-cors": "^6.4.5",
    "@middy/http-json-body-parser": "^6.4.5",
    "@middy/http-response-serializer": "^6.4.5",
    "@middy/validator": "^6.4.5"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.119",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  }
}
